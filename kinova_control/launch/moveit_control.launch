<launch>
    <arg name="file_name" default="j2s7s300_demo"/>
    <include file="$(find j2s7s300_moveit_config)/launch/$(arg file_name).launch"/>

    <arg name="control_client" default="pick_place_client.py"/>
    <arg name="control_server" default="pick_place_server2.py"/>

    <node name="arm_action_client" pkg="kinova_control" type="$(arg control_client)" respawn="false" output="screen">
    </node>
    <node name="arm_action_server" pkg="kinova_control" type="$(arg control_server)" respawn="true" output="screen">
    </node>
</launch>